{
  "name": "secretsfoundry",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "bin": {
    "secretsfoundry": "./cli/index.js"
  },
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii --watch",
    "package": "jsii-pacmak",
    "start:cli": "nodemon --watch 'cli/**/*.ts' --exec 'ts-node' cli/index.ts",
    "build:cli": "tsc -p .",
    "local": "npm i -g",
    "test": "npx ts-node./index.ts"
  },
  "keywords": [],
  "author": "AUTHOR",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.10.2",
    "jsii": "^1.35.0",
    "jsii-pacmak": "^1.35.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/innoavator/secretsfoundry.git"
  },
  "stability": "stable",
  "types": "./lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "servicefoundry.secretsfoundry",
        "module": "servicefoundry.secretsfoundry"
      }
    }
  },
  "homepage": "https://github.com/innoavator/secretsfoundry.git",
  "dependencies": {
    "aws-sdk": "^2.999.0",
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "figlet": "^1.5.2"
  },
  "bundledDependencies": [
    "chalk",
    "commander",
    "dotenv-flow",
    "figlet",
    "aws-sdk"
  ]
}